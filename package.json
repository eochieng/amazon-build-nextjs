{
  "name": "with-redux-toolkit",
  "version": "1.0.0",
  "homepage": "https://eochieng.github.io/amazon-build-nextjs",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier --write . --config .prettierrc",
    "dev": "yarn run lint && next",
    "build": "next build",
    "start": "next start",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@heroicons/react": "v1",
    "@reduxjs/toolkit": "1.5.0",
    "@tailwindcss/line-clamp": "^0.2.0",
    "firebase": "^8.6.1",
    "firebase-admin": "^9.8.0",
    "next": "latest",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^5.0.0",
    "postcss": "^8.2.15",
    "prettier": "2.8.4",
    "tailwindcss": "^2.1.2"
  }
}
